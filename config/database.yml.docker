default: &default
  adapter: postgresql
  encoding: unicode
  username: postgres
  password:
  host: postgres
  port: 5432
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>

development:
  <<: *default
  database: emres_development

test:
  <<: *default
  database: emres_test

production:
  <<: *default
  database: emres_production
